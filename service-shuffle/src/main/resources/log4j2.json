{
  "configuration": {
    "status": "WARN",
    "appenders": {
      "console": {
        "name": "Console",
        "type": "Console",
        "layout": {
          "type": "PatternLayout",
          "pattern": "%d{yyyy-MM-dd HH:mm:ss} [%t] %-5level %logger{36} - %msg%n"
        }
      }
    },
    "loggers": {
      "root": {
        "level": "info",
        "appenderRefs": [
          {
            "ref": "Console"
          }
        ]
      },
      "logger": {
        "name": "bgr.matrixee",
        "level": "debug",
        "additivity": false,
        "appenderRefs": [
          {
            "ref": "Console"
          }
        ]
      }
    }
  }
}
